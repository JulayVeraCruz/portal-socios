@model IEnumerable<PortalSocios.Models.Quotas>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Categoria.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Montante)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ano)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Periodicidade)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Categoria.Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Montante)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ano)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Periodicidade)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.QuotaID }) |
            @Html.ActionLink("Details", "Details", new { id=item.QuotaID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.QuotaID })
        </td>
    </tr>
}

</table>
